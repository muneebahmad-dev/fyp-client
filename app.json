{
  "expo": {
    "name": "e-photocopier",
    "slug": "e-photocopier",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-ios.png",
    "splash": {
      "image": "./assets/photo-blue.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.muneebahmad23.ephotocopier"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/photo-blue.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.muneebahmad23.ephotocopier"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
